[user]
	name = username
	email = username@email.com
[core]
	editor = mvim -f
        quotepath = false
	excludesfile = ~/.gitignore
[merge]
        log = true
[color]
        ui = auto
        interactive = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = magenta red bold
        new = green bold
[github]
        user = username
        password = password
	token = public_key
[push]
	default = current
